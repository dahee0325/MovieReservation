<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.reserve.manager.dao.ManagerDaoInterface">

	<!-- 영화 정보 리스트 -->
	<select id="movieList" resultType="com.project.reserve.manager.domain.MovieData">
		select midx,mName,mRunTime from movie
	</select>
	
	<!-- 극장 정보 리스트 -->
	<select id="cinemaList" resultType="com.project.reserve.manager.domain.ManagerCinemaData">
		select cidx,cName,cSeatCnt from cinema
	</select>
	
</mapper>


 
 